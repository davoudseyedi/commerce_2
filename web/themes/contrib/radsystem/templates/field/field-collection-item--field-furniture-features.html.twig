{#
/**
 * @file
 * Default theme implementation for field collection items.
 *
 * Available variables:
 * - content: An array of comment items. Use {{ content }} to print them all,
 *   or print a subset such as {{ content.field_example }}. Use
 *   {% hide(content.field_example) %} to temporarily suppress the printing
 *   of a given element.
 * - title: The (sanitized) field collection item label.
 * - url: Direct url of the current entity if specified.
 * - page: Flag for the full page state.
 * - attributes: HTML attributes for the surrounding element.
 *    Attributes include the 'class' information.
 * - content_attributes: HTML attributes for the content element.
 *
 * @see template_preprocess()
 * @see template_preprocess_entity()
 *
 * @ingroup themeable
 */
#}
{%
  set classes = [
  'field-collection-item',
  'field-collection-item--name-' ~ field_collection_item.name|clean_class,
  'field-collection-item--view-mode-' ~ field_collection_item.view_mode|clean_class,
]
%}
<div{{ attributes.addClass(classes) }}>
  <div class="content"{{ content_attributes }}>
    {#    {{ content }}#}
    <table class="table table-striped">
      <tbody>
      {% if content.field_body_type.0 %}
        <tr>

          <td>{{ content.field_body_type['#title'] }}</td>
          <td>{{ content.field_body_type.0 }}</td>

        </tr>
      {% endif %}
      {% if content.field_field_back_dimension2.0%}
        <tr>

          <td>{{ content.field_field_back_dimension2['#title'] }}</td>
          <td>{{ content.field_field_back_dimension2.0 }}</td>

        </tr>
      {% endif %}
      {% if content.field_main_dimension.0  %}
        <tr>

          <td>{{ content.field_main_dimension['#title'] }}</td>
          <td>{{ content.field_main_dimension.0 }}</td>

        </tr>
      {% endif %}
      {% if  content.field_main_dimension2.0  %}
        <tr>

          <td>{{ content.field_main_dimension2['#title'] }}</td>
          <td>{{ content.field_main_dimension2.0 }}</td>

        </tr>
      {% endif %}
      {% if content.field_bottom_dimension.0 %}
        <tr>

          <td>{{ content.field_bottom_dimension['#title'] }}</td>
          <td>{{ content.field_bottom_dimension.0 }}</td>

        </tr>
      {% endif %}
      {% if content.field_weight_size.0 %}
        <tr>

          <td>{{ content.field_weight_size['#title'] }}</td>
          <td>{{ content.field_weight_size.0 }}</td>

        </tr>
      {% endif %}
      {% if content.field_other_option1.0 %}
        <tr>

          <td>{{ content.field_other_option1['#title'] }}</td>
          <td>{{ content.field_other_option1.0 }}</td>

        </tr>
      {% endif %}
      {% if  content.field_back_dimension.0 %}
        <tr>

          <td>{{ content.field_back_dimension['#title'] }}</td>
          <td>{{ content.field_back_dimension.0 }}</td>

        </tr>
      {% endif %}
    
      </tbody>
    </table>
  </div>
</div>
