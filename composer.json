{
    "name": "drupalcommerce/project-base",
    "description": "Commerce 2.x project template",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "commerce_base": {
            "type": "vcs",
            "url": "https://github.com/drupalcommerce/commerce_base"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0.8",
        "ext-curl": "*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/ace_editor": "^1.1",
        "drupal/adminimal_admin_toolbar": "^1.10",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advanced_text_formatter": "^1.0",
        "drupal/anchor_link": "^1.6",
        "drupal/autocomplete_deluxe": "^1.0@beta",
        "drupal/better_exposed_filters": "^4.0@alpha",
        "drupal/betterlogin": "^1.3",
        "drupal/block_class": "^1.1",
        "drupal/block_field": "^1.0@alpha",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bxslider": "^1.0@beta",
        "drupal/captcha": "^1.0@beta",
        "drupal/chosen": "^2.9",
        "drupal/color_field": "^2.0",
        "drupal/colorbox": "^1.4",
        "drupal/commerce": "^2.0",
        "drupal/commerce_bulk": "^1.0@beta",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/context": "^4.0@beta",
        "drupal/core": "^8.8.0",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drupal/crop": "^1.5",
        "drupal/ctools": "^3.2",
        "drupal/devel_entity_updates": "^1.0",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/ds": "^3.5",
        "drupal/dynamic_entity_reference": "^1.8",
        "drupal/embed": "^1.3",
        "drupal/entity_browser": "^2.2",
        "drupal/entity_browser_enhanced": "^1.0@RC",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_embed": "^1.0",
        "drupal/extlink": "^1.1",
        "drupal/fac": "^1.3",
        "drupal/facets": "^1.4",
        "drupal/facets_pretty_paths": "^1.0",
        "drupal/fast_404": "^1.0@alpha",
        "drupal/features": "^3.8",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/field_collection_table_d8": "1.x-dev",
        "drupal/field_group": "^1.0",
        "drupal/field_timer": "^1.1",
        "drupal/file_entity": "^2.0@beta",
        "drupal/fivestar": "1.x-dev",
        "drupal/flag": "^4.0@beta",
        "drupal/flood_unblock": "^1.5",
        "drupal/focal_point": "^1.2",
        "drupal/google_analytics": "^3.0",
        "drupal/google_analytics_reports": "^3.0@beta",
        "drupal/google_tag": "^1.3",
        "drupal/image_resize_filter": "^1.0",
        "drupal/image_widget_crop": "^2.2",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imagemagick": "^2.6",
        "drupal/length_indicator": "^1.0",
        "drupal/libraries": "^3.0@alpha",
        "drupal/masonry": "^1.0@RC",
        "drupal/masonry_views": "^1.0@RC",
        "drupal/maxlength": "^1.0@beta",
        "drupal/media_entity_instagram": "^2.0",
        "drupal/metatag": "^1.11",
        "drupal/module_filter": "^3.1",
        "drupal/owlcarousel": "^1.0@alpha",
        "drupal/paragraphs": "^1.10",
        "drupal/paragraphs_edit": "^2.0@alpha",
        "drupal/pathauto": "^1.6",
        "drupal/pathologic": "^1.0@alpha",
        "drupal/r4032login": "^1.1",
        "drupal/redirect": "^1.5",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api": "^1.0",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplenews": "^1.0@beta",
        "drupal/smtp": "^1.0@beta",
        "drupal/swiftmailer": "^1.0",
        "drupal/taxonomy_access_fix": "^2.7",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/taxonomy_menu_ui": "^1.1",
        "drupal/token": "^1.5",
        "drupal/tvi": "^1.0@beta",
        "drupal/vartheme_bs4": "^6.5",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^3.3",
        "drupal/views_infinite_scroll": "^1.6",
        "drupal/viewsreference": "^1.4",
        "drupal/we_megamenu": "^1.7",
        "drupal/webform": "^5.6",
        "drupal/yoast_seo": "^1.5",
        "drupalcommerce/commerce_base": "dev-8.x-1.x",
        "oomphinc/composer-installers-extender": "^1.1",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
